{
  "name": "contributte/framex-skeleton",
  "description": "Fast API skeleton based on Framework X and Nette (@nette) by @f3l1x.",
  "keywords": [
    "nette",
    "skeleton",
    "project",
    "framex",
    "frameworkx",
    "api",
    "controller"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "contributte/kernel": "^0.1.0",
    "contributte/framex": "^0.1.0",
    "contributte/utils": "^0.6.0"
  },
  "require-dev": {
    "contributte/qa": "^0.3",
    "contributte/dev": "^0.4",
    "contributte/phpstan": "^0.1",
    "contributte/tester": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
